page {
  background: rgba(245, 245, 245, 1);
}

.inf{
  margin: 25rpx;
  line-height:50rpx;
  text-align:left;
  font-size: 30rpx;
}

.infz{
  margin: 25rpx;
  line-height:50rpx;
